rev && <button className=\"w-12 absolute -left-6 hover:scale-[1.06] z-10 top-1/2 -translate-y-1/2 h-12 rounded-full bg-white text-black flex items-center justify-center\" onClick={slideFavoritesPrev}><Icon name=\"prev\" size={24} /></button>}\n\t\t\t\t\t{next && <button className=\"w-12 absolute -right-6 hover:scale-[1.06] z-10 top-1/2 -translate-y-1/2 h-12 rounded-full bg-white text-black flex items-center justify-center\"  onClick={slideFavoritesNext}><Icon name=\"next\" size={24} /></button>}\n\t\t\t\t\t<ScrollContainer\n\t\t\t\t\t\tinnerRef={favoritesRef}\n\t\t\t\t\t\tclassName=\"flex scrollable overflow-x gap-x-6\"\n\t\t\t\t\t>\n\t\t\t\t\t\t{favoriteCategories.map((category, index) => <WideCategory key={index} category={category}/>)}\n\t\t\t\t\t</ScrollContainer>\n\t\t\t\t</div>\n\t\t\t</section>\n\n\t\t\t<section>\n\t\t\t\t<Title title=\"Hepsine gz at\" />\n\t\t\t\t<div className=\"grid grid-cols-5 gap-6\">\n\t\t\t\t\t{categories.map((category, index) => <Category key={index} category={category}/>)}\n\t\t\t\t</div>\n\t\t\t</section>\n\t\t</>\n\t)\n}\n\nexport default Search\n","function Collection() {\n\treturn (\n\t\t<div>\n\t\t\tYour collection\n\t\t</div>\n\t)\n}\n\nexport default Collection","import { Switch, Route } from 'react-router-dom';\nimport Navbar from 'components/Navbar'\n\nimport Home from \"views/Home\"\nimport Search from \"views/Search\"\nimport Collection from \"views/Collection\"\n\nfunction Content() {\n\treturn (\n\t\t<main className=\"flex-auto overflow-auto\">\n\t\t\t<Navbar />\n\t\t\t<div className=\"px-8 py-5\">\n\t\t\t\t<Switch>\n\t\t\t\t\t<Route exact path=\"/\">\n\t\t\t\t\t\t<Home />\n\t\t\t\t\t</Route>\n\t\t\t\t\t<Route path=\"/search\">\n\t\t\t\t\t\t<Search />\n\t\t\t\t\t</Route>\n\t\t\t\t\t<Route path=\"/collection\">\n\t\t\t\t\t\t<Collection />\n\t\t\t\t\t</Route>\n\t\t\t\t</Switch>\n\t\t\t</div>\n\t\t</main>\n\t)\n}\n\nexport default Content","import Sidebar from 'components/Sidebar'\nimport BottomBar from 'components/BottomBa